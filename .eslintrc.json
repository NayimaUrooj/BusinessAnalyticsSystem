{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
    "scripts": {
      "lint": "eslint ."
    },
  "rules": {
    "no-unused-expressions": "off",
    "no-sequences": "off",
    "no-func-assign": "off"
  }
}
